/* pages/common/materialData/index.wxss */

page, .page, .page__bd {
  height: 100%;
}

.page__bd {
  padding-bottom: 0;
}

.weui-tab__content {
  padding-top: 60px;
  text-align: center;
}

/* swiper-start */

.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #b6b6b6;
  text-align: center;
  line-height: 80rpx;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  color: #000;
}

.on {
  color: #58c5f8;
  border-bottom: 5rpx solid #58c5f8;
}

.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

swiper-item {
  overflow: auto;
}

/* swiper-end */

.waterPrint {
  position: absolute;
  /*bottom: 10rpx;
  right: 10rpx;*/
  top: -2rpx;
  right: 10rpx;
  width: 60rpx;
  height: 60rpx;
}

.collect {
  width: 50rpx;
  height: 50rpx;
}

.collectPosition {
  position: absolute;
  right: 10rpx;
  top: 50%;
  margin-top: -25rpx;
}


.ellipsis {
  width: 660rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}
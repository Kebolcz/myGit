<body>
    <div id="preloader">
        <div id="status">
            <p class="center-text">
                页面加载中...
            </p>
        </div>
    </div>
    <div class="all-elements">
        <div id="content" class="snap-content">
            <div class="container material-box" style="padding:0 0">
                <img src="~/Contents/images/car-booking.jpg" class="responsive-image" />

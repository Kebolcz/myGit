/* pages/locationGather/projectDetail/index.wxss */
.toggle{
  position: absolute;
  bottom: 0rpx;
  right: 20rpx;
}
.togglePng{
  width: 35rpx;
  height: 35rpx;
  transform: rotate(180deg);
}
.togglebtn{
    padding: 10rpx;
    border: 1px solid #e5e5e5;
    border-radius:8rpx;
    box-shadow: 0px 3px 5px #d8d8d8;
}

.productList{
  border-top:1px dashed #D9D9D9;
  border-bottom:1px dashed #D9D9D9;
}

.ellipsis {
  width: 600rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}